    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>homepages</category>
        <client_script/>
        <description/>
        <direct>false</direct>
        <endpoint>x_58872_needit_jelly_world.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	<j:set var="jvar_count" value="5"/>
	<j:set var="jvar_isReady" value="true"/>
	<h1>Problems</h1>
	<g:evaluate>
		var words = ['Hello', 'World', 'Jelly'];
	</g:evaluate>
	
	<g2:evaluate>
		var problems = new GlideRecord('x_58872_needit_needit2');
		var users=new GlideRecord('sys_user');
		users.query();
		problems.query();
		gs.debug(">>>>>>>>>>>>>Debug logging from jelly sciprt");
	</g2:evaluate>
	
	<!-- Print list from the collection -->
	<g:evaluate>
		//Here is the another commnet
	</g:evaluate>
	<j:forEach var="jvar_word2" items="${words}">
		<p> ${jvar_word2} </p>
	</j:forEach>

    <h2>List of needit items $[problems.getRowCount()]</h2>
	<ul>
    <j2:while test="$[problems.next()]">
		<li> $[problems.getValue('name')]</li>
	</j2:while>
		
    <h2>List of needit items $[users.getRowCount()]</h2>
		<ul>
		<j2:while test="$[users.next()]">
			<li>$[users.first_name]
				$[users.last_name]
				<b>($[users.email])</b>
			</li>
		</j2:while>
		</ul>
	</ul>
									 
</j:jelly>]]></html>
